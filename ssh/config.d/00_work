# vim: set ft=sshconfig:
CanonicalDomains int.ftdna.com unx.ftdna.com
CanonicalizePermittedCNAMEs *.*.ftdna.com:*.*.ftdna.com

Host *.ftdna.com
  User baot
  IdentityFile ~/.ssh/gpg-bao-gxg.pub
  ForwardAgent yes

Host gh-bao-gxg
  HostName github.com
  IdentityFile ~/.ssh/gpg-bao-gxg.pub
  IdentityFile ~/.ssh/id_rsa.work
